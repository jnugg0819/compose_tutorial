<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.composeexample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeExample">
        <activity
            android:name=".ComposeList"
            android:exported="true"
            android:label="@string/title_activity_compose_list"
            android:theme="@style/Theme.ComposeExample.NoActionBar" />
        <activity
            android:name=".MaterialCompose"
            android:exported="true"
            android:label="@string/title_activity_material_compose"
            android:theme="@style/Theme.ComposeExample.NoActionBar" />
        <activity
            android:name=".ComposeModifier"
            android:exported="true"
            android:label="@string/title_activity_compose_modifier"
            android:theme="@style/Theme.ComposeExample.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeExample.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>